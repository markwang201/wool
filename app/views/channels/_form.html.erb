<%= simple_form_for(@channel) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :qq_group %>
    <%= f.input :qq_group_name %>
    <%= f.input :group_master_qq %>
    <%= f.input :phone_number %>
    <%= f.input :master_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
